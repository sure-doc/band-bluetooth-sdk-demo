{
  "name": "@sport-app/family",
  "version": "0.0.1",
  "private": true,
  "description": "family",
  "workspaces": {
    "packages": [
      "modules/band-bluetooth-sdk",
      "modules/packages/eslint-config-my",
      "modules/packages/eslint-config-my-taro",
      "modules/packages/prettier-config",
      "modules/packages/taro-app-config",
      "modules/packages/taro-assets",
      "modules/packages/taro-config"
    ]
  },
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "app-config": "ts-node ./scripts/app-config.ts",
    "app-config:watch": "cross-env APP_CONFIG_WATCH=true run-s app-config",
    "taro:weapp": "taro build --type weapp",
    "taro-watch:weapp": "taro build --type weapp --watch",
    "taro:h5": "taro build --type h5",
    "taro-watch:h5": "taro build --type h5 --watch",
    "dev:weapp": "cross-env MY_DEV=true run-s app-config __dev:weapp",
    "__dev:weapp": "run-p app-config:watch taro-watch:weapp",
    "dev:h5": "cross-env MY_DEV=true run-s app-config __dev:h5",
    "__dev:h5": "run-p app-config:watch taro-watch:h5",
    "dev:h5-poster": "cross-env MY_POSTER=true run-s dev:h5",
    "dev-prod:weapp": "cross-env NODE_ENV=production run-s dev:weapp",
    "build:weapp": "run-s lint check-ts __build-taro:weapp",
    "__build-taro:weapp": "run-s app-config taro:weapp",
    "build:h5": "run-s lint check-ts __build-taro:h5",
    "__build-taro:h5": "run-s app-config taro:h5",
    "build:h5-poster": "cross-env MY_POSTER=true run-s build:h5",
    "lint": "node scripts/lint.js",
    "eslint": "eslint --no-error-on-unmatched-pattern --ext .js,.jsx,.ts,.tsx src",
    "check-ts": "tsc --noEmit"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@next/env": "13.3.0",
    "@tarojs/components": "3.6.5",
    "@tarojs/helper": "3.6.5",
    "@tarojs/plugin-framework-react": "3.6.5",
    "@tarojs/plugin-platform-h5": "3.6.5",
    "@tarojs/plugin-platform-weapp": "3.6.5",
    "@tarojs/react": "3.6.5",
    "@tarojs/runtime": "3.6.5",
    "@tarojs/shared": "3.6.5",
    "@tarojs/taro": "3.6.5",
    "@types/react-transition-group": "^4.4.5",
    "aegis-mp-sdk": "^1.37.1",
    "aegis-web-sdk": "^1.37.1",
    "ahooks": "^3.7.6",
    "band-bluetooth-sdk": "workspace:*",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "loglevel": "^1.8.1",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "nanoid": "^4.0.2",
    "query-string": "^7.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.0",
    "taro-code": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@my/prettier-config": "workspace:*",
    "@my/taro-app-config": "workspace:*",
    "@my/taro-assets": "workspace:*",
    "@my/taro-config": "workspace:*",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tarojs/cli": "3.6.5",
    "@tarojs/webpack5-runner": "3.6.5",
    "@types/react": "^18.0.37",
    "@types/webpack-env": "^1.18.0",
    "babel-preset-taro": "3.6.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-taro": "3.6.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "react-refresh": "^0.14.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "yarn@3.5.0",
  "resolutions": {
    "@swc/core": "1.3.42",
    "postcss-pxtransform@3.6.5": "patch:postcss-pxtransform@npm%3A3.6.5#./modules/.yarn/patches/postcss-pxtransform-npm-3.6.5-5be8eb6cf8.patch",
    "@tarojs/taro-h5@3.6.5": "patch:@tarojs/taro-h5@npm%3A3.6.5#./modules/.yarn/patches/@tarojs-taro-h5-npm-3.6.5-787b86f873.patch",
    "@tarojs/taro-loader@3.6.5": "patch:@tarojs/taro-loader@npm%3A3.6.5#./modules/.yarn/patches/@tarojs-taro-loader-npm-3.6.5-912c9469df.patch",
    "@tarojs/api@3.6.5": "patch:@tarojs/api@npm%3A3.6.5#./modules/.yarn/patches/@tarojs-api-npm-3.6.5-dd354ad9df.patch",
    "@tarojs/webpack5-runner@3.6.5": "patch:@tarojs/webpack5-runner@npm%3A3.6.5#./modules/.yarn/patches/@tarojs-webpack5-runner-npm-3.6.5-c908ea8ef0.patch"
  }
}
